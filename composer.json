{
    "name": "weijukeji/laravel-dictionary",
    "description": "Laravel Dictionary package for managing dictionary categories and items with caching support",
    "keywords": ["laravel", "dictionary", "cache", "api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "WeiJuKeJi",
            "email": "dev@weijukeji.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0 || ^12.0",
        "tucker-eric/eloquentfilter": "^3.5"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "WeiJuKeJi\\LaravelDictionary\\": "src/",
            "WeiJuKeJi\\LaravelDictionary\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeiJuKeJi\\LaravelDictionary\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WeiJuKeJi\\LaravelDictionary\\DictionaryServiceProvider"
            ],
            "aliases": {
                "Dict": "WeiJuKeJi\\LaravelDictionary\\Facades\\Dict"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
